Public Sub takim()

    Dim tx As Integer

    temiz
    m = Split(yazi, "   ")
    For Each stre In m
        If stre <> ""
            If stre Like "+*" Then stre = Right$(stre, -1)
            tx = tx + 1
            If tx = 1 Then sonuc1 = Trim$(stre)
            If tx = 2 Then sonuc2 = Trim$(stre)
            If tx = 3 Then sonuc3 = Trim$(stre)
        End If
    Next
    If sonuc2 = "0.0000000E+00" Then sonuc2 = ""
    If sonuc3 = "0.0000000E+00" Then sonuc3 = ""
    If sonuc2 = "-NAN" Then sonuc2 = ""
    If sonuc3 = "-NAN" Then sonuc2 = ""
    Return
Catch
ilk.hatayaz("takim " & Error.Text, Error.Where & "--" & Error.code)
Return
End

