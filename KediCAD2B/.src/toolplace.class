Public Sub Button1_Click()
Dim myFile As File
myFile = Open System.User.Home &/ ".KediCAD/CNC/1" For Create 
      Print #myFile, "X" & TextBox1.Text
      Print #myFile, "Y" & TextBox2.Text
      Print #myFile, "Z" & TextBox3.Text
      Print #myFile, TextBox4.Text
      Print #myFile, TextBox5.Text
      Print #myFile, TextBox6.Text
      Print #myFile, TextBox7.Text
Close myFile 
End

